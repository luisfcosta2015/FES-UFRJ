<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>Parameters</servlet-name>
        <jsp-file>/parameters.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>Parameters</servlet-name>
        <url-pattern>/parameters</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ParametersTest</servlet-name>
        <servlet-class>ParametersTest</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ParametersTest</servlet-name>
        <url-pattern>/parametersTest</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>TEST</servlet-name>
        <servlet-class>StartServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TEST</servlet-name>
        <url-pattern>/st</url-pattern>
    </servlet-mapping>

    <resource-ref>
        <description>postgreSQL Datasource</description>
        <res-ref-name>jdbc/postgres</res-ref-name>
        <res-type>javax.sql.DataSource</res-type>
        <res-auth>Container</res-auth>
    </resource-ref>
    
</web-app>